<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="352.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AdminController">
   <children>
      <TextField fx:id="name" layoutX="76.0" layoutY="85.0" promptText="name" />
      <TextField fx:id="username" layoutX="76.0" layoutY="144.0" promptText="username" />
      <TextField fx:id="password" layoutX="76.0" layoutY="205.0" promptText="password" />
      <Label layoutX="101.0" layoutY="23.0" text="Add User">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <CheckBox fx:id="isAdminBox" layoutX="76.0" layoutY="259.0" mnemonicParsing="false" text="Is admin?" />
      <Button fx:id="addUserButton" onAction="#addUserButton" layoutX="75.0" layoutY="299.0" mnemonicParsing="false" text="Add User" />
      <Button fx:id="cancel" onAction="#closeAddUser" layoutX="150.0" layoutY="299.0" mnemonicParsing="false" text="Cancel" />
   </children>
</AnchorPane>
