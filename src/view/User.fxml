<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.UserController">
    <children>
      <HBox alignment="CENTER" layoutY="42.0" minWidth="600.0" prefHeight="25.0" prefWidth="600.0" spacing="15.0">
         <children>
              <Button minWidth="100.0" mnemonicParsing="false" onAction="#newAlbum" prefWidth="100.0" text="New Album" />
              <Button minWidth="100.0" mnemonicParsing="false" onAction="#rename" prefWidth="100.0" text="Rename Album" />
              <Button minWidth="100.0" mnemonicParsing="false" onAction="#delete" prefWidth="100.0" text="Delete Album" />
            <Button minWidth="100.0" mnemonicParsing="false" onAction="#open" prefWidth="100.0" text="Open Album" />
              <Button minWidth="100.0" mnemonicParsing="false" onAction="#logout" prefWidth="100.0" text="Logout" />
         </children>
      </HBox>

        <TableView fx:id="albumTable" layoutY="78.0" minWidth="600.0" prefHeight="484.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="albumNameCol" prefWidth="311.0" text="Album Name" />
                <TableColumn fx:id="photoCountCol" minWidth="0.0" prefWidth="92.0" text="Photo Count" />
                <TableColumn fx:id="dateRangeCol" prefWidth="195.0" resizable="false" text="Dates" />
            </columns>
        </TableView>
        <Label fx:id="usernameLabel" layoutX="29.0" layoutY="7.0" text="User Name">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Pane layoutX="1.0" layoutY="562.0" prefHeight="37.0" prefWidth="600.0">
            <children>
                <TextField layoutY="5.0" prefHeight="27.0" prefWidth="203.0" promptText="Date" />
                <TextField layoutX="207.0" layoutY="5.0" prefHeight="27.0" prefWidth="138.0" promptText="TagName" />
                <TextField layoutX="349.0" layoutY="5.0" prefHeight="27.0" prefWidth="138.0" promptText="TagValue" />
                <Label layoutX="345.0" layoutY="10.0" prefHeight="17.0" prefWidth="4.0" text=":" />
                <Button layoutX="492.0" layoutY="5.0" mnemonicParsing="false" text="Search Photos" />
            </children>
        </Pane>
    </children>
</AnchorPane>
