<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxHeight="300.0" maxWidth="400.0" minHeight="350.0" minWidth="400.0" prefHeight="300.0" prefWidth="400.0" spacing="20.0" style="-fx-background-color: '#FFFFFF';" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LoginController">
    <children>
        <Label text="Login">
            <font>
                <Font size="43.0" />
            </font>
        </Label>
        <Label text="Username" />
        <TextField fx:id="usernameInput" prefHeight="27.0" />
        <Label text="Password" />
        <TextField fx:id="passwordInput" prefHeight="27.0" />
        <Button mnemonicParsing="false" onAction="#submit" prefWidth="100.0" style="-fx-background-color: '#E0E0E0';" text="Login" />
    </children>
    <padding>
        <Insets left="50.0" right="50.0" />
    </padding>
</VBox>
