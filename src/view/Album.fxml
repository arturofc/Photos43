<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="600.0" prefWidth="900.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AlbumController">
   <top>
      <HBox alignment="CENTER_RIGHT" prefHeight="25.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label id="imageListAlbumNameLabel" fx:id="albumLabel" cache="true" cacheHint="QUALITY" prefWidth="310.0" text="Album Name">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="35.0" />
               </HBox.margin>
            </Label>
            <Button mnemonicParsing="false" onAction="#pickImage" prefHeight="39.0" prefWidth="50.0" text="New" />
            <Button mnemonicParsing="false" onAction="#details" prefHeight="39.0" prefWidth="75.0" text="Details" />
            <Button mnemonicParsing="false" onAction="#slideshow" prefHeight="39.0" prefWidth="75.0" text="Slideshow" />
            <Button mnemonicParsing="false" onAction="#delete" prefHeight="39.0" prefWidth="75.0" text="Delete" />
            <MenuButton fx:id="moveToMenu" mnemonicParsing="false" prefHeight="39.0" prefWidth="100.0" text="Move to">
              <items>
              </items>
            </MenuButton>
            <MenuButton fx:id="copyToMenu" mnemonicParsing="false" prefHeight="39.0" prefWidth="100.0" text="Copy to">
              <items>
              </items>
            </MenuButton>
            <Button mnemonicParsing="false" onAction="#logout" prefHeight="39.0" prefWidth="75.0" text="Logout" />
            <Button mnemonicParsing="false" onAction="#back" prefHeight="39.0" prefWidth="50.0" text="Back" />
         </children>
         <padding>
            <Insets top="15.0" />
         </padding>
      </HBox>
   </top>
   <center>
      <TilePane fx:id="imageTable" prefWidth="900.0" style="-fx-background-color: '#FFFFFF';" vgap="15.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
